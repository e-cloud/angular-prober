{
  "name": "Angular Prober",
  "version": "0.2.0",
  "description": "Extends the Developer Tools, adding tools for inspecting component context of Angular applications.",
  "devtools_page": "devtoolsBackground.html",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "icons": {
    "16": "img/angular.png",
    "48": "img/angular.png",
    "128": "img/angular.png"
  },
  "page_action": {
    "default_icon": {
      "19": "img/angular.png",
      "38": "img/angular.png"
    },
    "default_title": "Angular Prober"
  },
  "minimum_chrome_version": "45"
}